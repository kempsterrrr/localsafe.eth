{
  "name": "localsafe.eth",
  "version": "0.2.6",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write \"app/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"app/**/*.{ts,tsx,js,jsx}\"",
    "localsafe": "next build && npx serve out -p 3000",
    "anvil": "anvil --load-state ./anvil-safe-state.json --block-time 1",
    "test:e2e": "bash tests/scripts/start-anvil-and-test.sh",
    "test:clean": "pkill -9 ^next-server || true; pkill -9 ^anvil",
    "synpress:build": "anvil --chain-id 31337 > /dev/null 2>&1 & ANVIL_PID=$! && sleep 2 && xvfb-run pnpm exec synpress --force && kill $ANVIL_PID",
    "postinstall": "git config core.hooksPath .githooks && echo 'Git hooks installed' || true"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@reown/walletkit": "^1.3.0",
    "@safe-global/protocol-kit": "^6.1.0",
    "@safe-global/safe-deployments": "^1.37.44",
    "@tanstack/react-query": "^5.85.5",
    "@wagmi/connectors": "^5.9.8",
    "@wagmi/core": "^2.20.1",
    "@walletconnect/core": "^2.22.4",
    "@walletconnect/types": "^2.22.4",
    "ethers": "^6.15.0",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "^7.9.5",
    "viem": "~2.35.1",
    "wagmi": "^2.16.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.5.0",
    "@playwright/test": "1.48.2",
    "@synthetixio/synpress": "^4.1.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "daisyui": "^5.0.54",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "permaweb-deploy": "2.5.1",
    "playwright-core": "1.48.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}